    "@sanity/default-login": "^2.13.1",
    "@sanity/desk-tool": "^2.13.1",
    "@sanity/vision": "^2.13.1",
    "babel-preset-gatsby": "^1.5.0",
    "date-fns": "^2.21.3",
    "postcss": "^8.2.15",
    "prop-types": "^15.7",
    "react": "^17.0",
    "react-dom": "^17.0",
    "react-feather": "^2.0.9",
    "sanity-plugin-media": "^1.3.1"
  },
  "devDependencies": {
    "@sanity/cli": "^2.13.1"
