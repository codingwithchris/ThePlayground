{
  "name": "@nerve/cms",
  "private": true,
  "version": "1.3.0",
  "description": "CMS that powers The Nerve web app",
  "main": "package.json",
  "author": "Chris Hahn <chris+digital@chrishahn.me>",
  "license": "MIT",
  "scripts": {
