    "@sanity/default-login": "^2.29.8",
    "@sanity/desk-tool": "^2.29.8",
    "@sanity/vision": "^2.29.8",
    "babel-preset-gatsby": "^2.11.1",
    "date-fns": "^2.28.0",
    "postcss": "^8.4.12",
    "prop-types": "^15.8",
    "react": "^18.0",
    "react-dom": "^18.0",
    "react-feather": "^2.0.9",
    "sanity-plugin-media": "^1.4.4",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@sanity/cli": "^2.29.8"
