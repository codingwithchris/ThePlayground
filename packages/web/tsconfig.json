{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"baseUrl": "",
		"paths": {
			"@app/*": [
				"./@app/*"
			],
			"@gatsby/*": [
				"./@gatsby/*"
			],
			"@netlify/*": [
				"./@netlify/*"
			],
			"@nerve/core/*": [
				"./src/core/*"
			],
			"@nerve/assets": [
				"./src/assets/"
			],
			"@nerve/domains/*": [
				"./src/domains/*"
			],
			"@nerve/shared/*": [
				"./src/shared/*"
			]
		}
	},
	"includes": [
		"src/**/*",
		"@app/**/*",
		"@gatsby/**/*",
		"@netlify/**/*",
		"gatsby-*.js"
	]
}