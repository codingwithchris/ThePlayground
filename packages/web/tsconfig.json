{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"baseUrl": "",
		"paths": {
			"@gatsby/*": [
				"./@gatsby/*"
			],
			"@web/core/*": [
				"./src/core/*"
			],
			"@web/assets": [
				"./src/assets/"
			],
			"@web/domains/*": [
				"./src/domains/*"
			],
			"@web/page-parts/*": [
				"./src/page-parts/*"
			],
			"@web/shared/*": [
				"./src/shared/*"
			],
			"@web/ui/core": [
				"./src/ui/components/core/"
			],
			"@web/ui/layout": [
				"./src/ui/components/layout/"
			],
			"@web/ui/molecules": [
				"./src/ui/components/molecules/"
			],
			"@web/ui/tokens": [
				"./src/ui/tokens/"
			],
			"@web/ui/themes": [
				"./src/ui/themes/"
			]
		}
	},
	"includes": [
		"types/**/*",
		"src/**/*",
		"@gatsby/**/*",
		"gatsby-*.js"
	]
}