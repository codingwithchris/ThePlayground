	"scripts": {
		"clean": "gatsby clean",
		"start": "gatsby develop",
		"start:clean": "npm run clean && gatsby develop",
		"netlify:start": "npm run clean && netlify dev",
		"netlify:build": "netlify build --context deploy-preview",
		"build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
		"serve": "npm run build && gatsby serve",
		"empty-cache": "npm run clean",
		"lint:js": "eslint . --cache --cache-location node_modules/.cache/eslint --ext js,jsx,ts,tsx",
		"lint:js:fix": "eslint . --cache --cache-location node_modules/.cache/eslint --ext js,jsx,ts,tsx --fix",
		"lint:clean": "rm -rf node_modules/.cache/eslint",
		"lint:css": "./node_modules/.bin/stylelint '**/*.css' --syntax css",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"browserslist": [
		"last 2 versions",
		"last 1 year",
		">0.5%",
		"not IE > 0",
		"not dead"
	]
